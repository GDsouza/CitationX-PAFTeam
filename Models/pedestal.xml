   </animation>

<!-- FLAPS -->

    <animation>
        <type>translate</type>
        <object-name>FlapLever</object-name>
		    <object-name>Flap.dwn</object-name>
		    <object-name>Flap.up</object-name>
        <property>controls/flight/flaps</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>0.0428</ind><dep>0.0117</dep></entry>
            <entry><ind>0.142</ind><dep>0.0275</dep></entry>
            <entry><ind>0.428</ind><dep>0.0475</dep></entry>
            <entry><ind>1.0</ind><dep>0.0724</dep></entry>
        </interpolation>
        <axis>
            <x>0.996</x>
            <y>0.0</y>
            <z>-0.087</z>
        </axis>
    </animation>

<animation>
    <type>pick</type>
    <object-name>Flap.dwn</object-name>
    <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>controls.flapsDown(1)</script>
        </binding>
    </action>
    <hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>Flaps down</tooltip-id>
				<label>Flaps : %s</label>
				<property>controls/flight/flaps-select</property>
				<mapping>nasal</mapping>
				<script>
					var modes = ['UP','SLATS','5 deg','15 deg','FULL'];
					return modes[arg[0]];
				</script>
			</binding>
		</hovered>

</animation>

<animation>
    <type>pick</type>
    <object-name>Flap.up</object-name>
    <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>controls.flapsDown(-1)</script>
        </binding>
    </action>
</animation>

<animation>
