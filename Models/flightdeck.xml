<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

    <path>flightdeck.ac</path>

    <model>
        <name>panel1</name>
        <path>Aircraft/CitationX/Models/panel.xml</path>
        <offsets>
            <x-m>-1.123</x-m>
            <y-m>0</y-m>
            <z-m>0.426</z-m>
            <pitch-deg>-15</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>pedestal</name>
        <path>Aircraft/CitationX/Models/pedestal.xml</path>
        <offsets>
            <x-m>-1.123</x-m>
            <y-m>0</y-m>
            <z-m>0.426</z-m>
        </offsets>
    </model>

    <model>
        <name>pilot-seat</name>
        <path>Aircraft/CitationX/Models/Seats/pilot-seat.xml</path>
        <offsets>
            <x-m> 0.00 </x-m>
            <y-m> 0.00 </y-m>
            <z-m> 0.00</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>copilot-seat</name>
        <path>Aircraft/CitationX/Models/Seats/copilot-seat.xml</path>
        <offsets>
            <x-m> 0.00 </x-m>
            <y-m> 0.00 </y-m>
            <z-m> 0.00</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>panel1</object-name>
        <object-name>pedestal</object-name>
        <condition>
            <property>sim/current-view/internal</property>
        </condition>
    </animation>

    <!-- Yokes -->

    <animation>
        <type>rotate</type>
        <object-name>L-yoke</object-name>
        <property>controls/flight/aileron</property>
        <factor>-30</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.375</y-m>
            <z-m>0.55</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>L-manche-yoke</object-name>
		    <object-name>L-yoke</object-name>
        <property>controls/flight/elevator</property>
        <factor>-4</factor>
        <center>
            <x-m>-0.85</x-m>
            <y-m>-0.375</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>R-yoke</object-name>
        <property>controls/flight/aileron</property>
        <factor>-30</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0.375</y-m>
            <z-m>0.55</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>R-manche-yoke</object-name>
		    <object-name>R-yoke</object-name>
        <property>controls/flight/elevator</property>
        <factor>-4</factor>
        <center>
            <x-m>-0.85</x-m>
            <y-m>0.375</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

	<animation>
		<type>select</type>
				<object-name>L-manche-yoke</object-name>
				<object-name>L-yoke</object-name>
		<condition>
				<property>sim/model/show-yoke_L</property>
		</condition>
	</animation>

	<animation>
   <type>select</type>
        <object-name>R-manche-yoke</object-name>
		    <object-name>R-yoke</object-name>
   <condition>
       <property>sim/model/show-yoke_R</property>
   </condition>
 </animation>

 <animation>
   <type>pick</type>
        <object-name>L-manche-yoke</object-name>
		    <object-name>L-yoke</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
       <command>property-assign</command>
       <property>sim/model/show-yoke_L</property>
				<value>0</value>
     </binding>
			<binding>	
				<command>nasal</command>
				<script>controls.pilots()</script>
			</binding>
   </action>
   <hovered>
     <binding>
        <command>set-tooltip</command>
        <tooltip-id>show-hide-yoke</tooltip-id>
        <label>Show/Hide Yoke</label>
     </binding>
   </hovered>
 </animation>

 <animation>
   <type>pick</type>
        <object-name>R-manche-yoke</object-name>
		    <object-name>R-yoke</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
       <command>property-assign</command>
       <property>sim/model/show-yoke_R</property>
				<value>0</value>
     </binding>
			<binding>	
				<command>nasal</command>
				<script>controls.pilots()</script>
			</binding>
   </action>
   <hovered>
     <binding>
        <command>set-tooltip</command>
        <tooltip-id>show-hide-yoke</tooltip-id>
        <label>Show/Hide Yoke</label>
     </binding>
   </hovered>
 </animation>

 <animation>
   <type>pick</type>
   <object-name>base-pied-yoke</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
       <command>property-assign</command>
       <property>sim/model/show-yoke_L</property>
				<value>1</value>
     </binding>
     <binding>
       <command>property-assign</command>
       <property>sim/model/show-yoke_R</property>
				<value>1</value>
     </binding>
			<binding>	
				<command>nasal</command>
				<script>controls.pilots()</script>
			</binding>
   </action>
   <hovered>
     <binding>
        <command>set-tooltip</command>
        <tooltip-id>show-hide-yoke</tooltip-id>
        <label>Show/Hide Yoke</label>
     </binding>
   </hovered>
 </animation>

    <!-- Pilots -->
  <model>
		<name>Captain</name>
    <path>Aircraft/CitationX/Models/Pilot/pilot.xml</path>
		<condition>
			<property>sim/model/show-pilot</property>
		</condition>
		<overlay>
	    <params>
				<texture>old.png</texture>
				<legs>
		    	<upper-animation>
						<offset-deg>90</offset-deg>
		    	</upper-animation>
		    		<lower-animation>
					<offset-deg>-90</offset-deg>
		    		</lower-animation>
				</legs>
	    </params>
		</overlay>
		<offsets>
	    <x-m>-0.57713 </x-m>
	    <y-m>-0.35988</y-m>
	    <z-m> 0.50333 </z-m>
		</offsets>
  </model>

  <model>
		<name>First officer</name>
      <path>Aircraft/CitationX/Models/Pilot/copilot.xml</path>
			<condition>
				<property>sim/model/show-copilot</property>
			</condition>
			<overlay>
	    	<params>
					<texture>default.png</texture>
					<legs>
		    		<upper-animation>
							<offset-deg>90</offset-deg>
		    		</upper-animation>
		    		<lower-animation>
							<offset-deg>-90</offset-deg>
		    		</lower-animation>
					</legs>
	    	</params>
			</overlay>
			<offsets>
	    	<x-m>-0.57713 </x-m>
	    	<y-m> 0.38295</y-m>
	    	<z-m> 0.50333 </z-m>
			</offsets>
  </model>

<!-- Instruments-->

    <animation>
        <type>rotate</type>
        <object-name>compassrose</object-name>
        <property>instrumentation/magnetic-compass/indicated-heading-deg</property>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
        <center>
            <x-m>-1.237</x-m>
            <y-m>0.00</y-m>
            <z-m>0.916</z-m>
        </center>
    </animation>
<!-- text -->

    <text>
        <name>APUrpm</name>
        <offsets>
            <x-m>-0.876</x-m>
            <y-m>0.700</y-m>
            <z-m>0.517</z-m>
        </offsets>
