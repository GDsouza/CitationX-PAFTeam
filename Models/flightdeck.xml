        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>xz-plane</axis-alignment>
        <type type="string">literal</type>
				<text type="string">READY TO LOAD</text>
        <format>%s</format>
        <font>helvetica_bold.txf</font>
        <character-size>0.0034</character-size>
        <character-aspect-ratio type="double">1.0</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
				<kerning>none</kerning>
    </text>

    <text>
        <name>APUbleed</name>
        <offsets>
            <x-m>-0.875</x-m>
            <y-m>0.698</y-m>
            <z-m>0.571</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>xz-plane</axis-alignment>
        <type type="string">literal</type>
				<text type="string">BLEED VALVE OPEN</text>
        <format>%s</format>
        <font>helvetica_bold.txf</font>
        <character-size>0.0031</character-size>
        <character-aspect-ratio type="double">1.0</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
				<kerning>none</kerning>
    </text>

    <text>
        <name>FLmeter</name>
        <offsets>
            <x-m>-0.842</x-m>
            <y-m>0.700</y-m>
            <z-m>0.626</z-m>
        </offsets>
        <alignment>right-center</alignment>
        <axis-alignment>xz-plane</axis-alignment>
        <type type="string">number-value</type>
        <property>instrumentation/clock/flight-meter-hour</property>
        <format>%4.1f</format>
        <font>helvetica_bold.txf</font>
        <character-size>0.009</character-size>
        <character-aspect-ratio type="double">1.0</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>

    <animation>
        <type>select</type>
        <object-name>APUrpm</object-name>
        <object-name>APUegt</object-name>
        <condition>
					<and>
            <property>controls/APU/master</property>
							<not>
            		<property>controls/APU/test</property>
							</not>
					</and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APUrpmTest</object-name>
        <object-name>APUegtTest</object-name>
        <condition>
            <property>controls/APU/test</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APUvolts</object-name>
        	<condition>
						<or>
            	<property>controls/APU/master</property>
            	<property>controls/APU/test</property>
						</or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APUready</object-name>
        	<condition>
						<and>
							<not>
								<property>controls/electric/APU-generator</property> 
							</not>
							<or>
							<equals>
            		<property>controls/APU/rpm</property>
									<value>1</value>
							</equals>
            	<property>controls/APU/test</property>
						</or>
					</and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APUbleed</object-name>
        	<condition>
						<or>
							<and>
								<greater-than>
									<property>controls/APU/bleed</property> 
										<value>0</value>
								</greater-than>
								<equals>
  	          		<property>controls/APU/rpm</property>
										<value>1</value>
								</equals>
							</and>
            	<property>controls/APU/test</property>
						</or>
        </condition>
    </animation>

<!-- hotspots -->

    <animation>
        <type>pick</type>
        <object-name>APU.master</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>controls/APU/master</property>
            </binding>
						<binding>
            	<command>property-toggle</command>
            	<property>sim/sound/click</property>
						</binding>
						<binding>
							<condition>
                <property>controls/APU/master</property>
							</condition>
								<command>property-assign</command>
                <property>controls/APU/battery</property>
                <value>24</value>
            </binding>
						<binding>
							<condition>
									<not>
	                	<property>controls/APU/master</property>
									</not>
						</condition>
								<command>property-assign</command>
                	<property>controls/APU/running</property>
                	<value>0</value>
						</binding>
						<binding>
							<condition>
								<not>
                	<property>controls/APU/master</property>
								</not>
							</condition>
								<command>property-assign</command>
                	<property>controls/APU/battery</property>
                	<value>0</value>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>APU-master-switch</tooltip-id>
                <label>APU-master: %s</label>
                <mapping>on-off</mapping>
            <property>controls/APU/master</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>APU.master</object-name>
        <property>controls/APU/master</property>
        <factor>30</factor>
        <center>
           <x-m>-0.830</x-m>
            <y-m>0.698</y-m>
            <z-m>0.434</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>APU.on_off</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>controls/APU/start-stop</property>
									<value>1.0</value>
 	          </binding>
						<binding>
            	<command>property-toggle</command>
            	<property>sim/sound/click</property>
						</binding>
            <binding>
							<condition>
								<and>
									<property>controls/APU/master</property>
									<property>controls/APU/start-stop</property>
								</and>
							</condition>
              <command>property-assign</command>
                <property>controls/APU/running</property>
                <value>1</value>
            </binding>
					<mod-up>
			       <binding>
         				<command>property-assign</command>
         					<property>controls/APU/start-stop</property>
         						<value>0</value>
       				</binding>
     			</mod-up>
       </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>APU-start-stop</tooltip-id>
                <label>APU-start-stop: %s</label>
                <mapping>heading</mapping>
            <property>controls/APU/start-stop</property>
            </binding>
        </hovered>

    </animation>

    <animation>
        <type>pick</type>
        <object-name>APU.on</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>controls/APU/start-stop</property>
									<value>1.0</value>
 	          </binding>
						<binding>
            	<command>property-toggle</command>
            	<property>sim/sound/click</property>
						</binding>
            <binding>
							<condition>
								<and>
									<property>controls/APU/master</property>
									<property>controls/APU/start-stop</property>
								</and>
							</condition>
              <command>property-assign</command>
                <property>controls/APU/running</property>
                <value>1</value>
            </binding>
					<mod-up>
			       <binding>
         				<command>property-assign</command>
         					<property>controls/APU/start-stop</property>
         						<value>0</value>
       				</binding>
     			</mod-up>
       </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>APU.off</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>controls/APU/start-stop</property>
									<value>-1</value>
 	          </binding>
						<binding>
            	<command>property-toggle</command>
            	<property>sim/sound/click</property>
						</binding>
            <binding>
              <command>property-assign</command>
                <property>controls/APU/running</property>
                <value>false</value>
            </binding> 
            <binding>
              <command>property-assign</command>
                <property>controls/electric/APU-generator</property>
                <value>false</value>
            </binding> 
            <binding>
              <command>property-assign</command>
                <property>controls/APU/battery</property>
                <value>24</value>
            </binding> 
					<mod-up>
			       <binding>
         				<command>property-assign</command>
         					<property>controls/APU/start-stop</property>
         						<value>0</value>
       				</binding>
     			</mod-up>
       </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>APU.on_off</object-name>
        <property>controls/APU/start-stop</property>
        <factor>30</factor>
        <center>
           <x-m>-0.830</x-m>
            <y-m>0.698</y-m>
            <z-m>0.434</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>APU.test</object-name>	
        <property>controls/APU/test</property>
				<factor>-0.005</factor>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

		<animation>
    	<type>pick</type>
    	<object-name>APU.test</object-name>
   		<action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
            <command>property-toggle</command>
            <property>sim/sound/click</property>
