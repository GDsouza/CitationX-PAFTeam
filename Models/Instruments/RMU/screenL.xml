<?xml version="1.0" encoding="ISO-8859-1"?>


<PropertyList>

<path>screenL.ac</path>

  <animation>
    <type>select</type>
    <condition>
      <property>systems/electrical/outputs/rmu1</property>
    </condition>
    <object-name>RMU.screenL</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>RMU.screenL</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/rmu</factor-prop>
    </emission>
  </animation>

</PropertyList>
