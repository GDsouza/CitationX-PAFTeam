<?xml version="1.0" encoding="ISO-8859-1"?>


<PropertyList>

<path>screenR.ac</path>

  <animation>
    <type>select</type>
    <condition>
      <property>systems/electrical/outputs/rmu2</property>
    </condition>
    <object-name>RMU.screenR</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>RMU.screenR</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/rmu[1]</factor-prop>
    </emission>
  </animation>

</PropertyList>
