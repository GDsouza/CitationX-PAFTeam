<!--
Citation X - Navigation display
-->

<PropertyList>

<path>screenL_B.ac</path>

    <animation>
        <type>material</type>
        <object-name>screenL_B</object-name>
        <condition>
						<or>
	           	<property>systems/electrical/left-emer-bus</property>
	           	<property>systems/electrical/left-bus-norm</property>
						</or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>controls/lighting/mfd-norm</factor-prop>
        </emission>
    </animation>

</PropertyList>
