<PropertyList>

  <path>cdu.ac</path>


  <animation>
    <type>select</type>
    <object-name>CDU.screenL</object-name>
    <object-name>CDU-lamp</object-name>
    <object-name>annun.1W</object-name>
    <object-name>annun.2A</object-name>
    <object-name>annun.3A</object-name>
    <object-name>annun.4W</object-name>
    <object-name>annun.5W</object-name>
    <object-name>annun.6W</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <greater-than>
          <property>controls/lighting/cdu</property>
          <value>0.02</value>
        </greater-than>
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.1W</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <property>instrumentation/cdu/fltplan</property>
        <property>autopilot/route-manager/active</property>
        <not-equals>
          <property>instrumentation/cdu/display</property>
          <value>FLT-PLAN[1]</value>
        </not-equals>
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.2A</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <or>
          <not>
            <property>systems/electrical/outputs/gps1</property>
          </not>
          <not>
            <property>systems/electrical/outputs/dme1</property>
          </not>
          <not>
            <property>systems/electrical/outputs/att-hdg1</property>
          </not>
          <not>
            <property>systems/electrical/outputs/att-hdg-aux1</property>
          </not>
        </or>
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.3A</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <equals> 
          <property>autopilot/settings/nav-source</property>
          <value>>FMS1</value>
        </equals> 
        <or>
          <not>
            <property>systems/electrical/outputs/gps1</property>
          </not>
          <not>
            <property>systems/electrical/outputs/dme1</property>
          </not>
          <not>
            <property>systems/electrical/outputs/att-hdg1</property>
          </not>
        </or>
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.4W</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <not-equals> 
          <property>instrumentation/cdu/alarms</property>
          <value>0</value>
        </not-equals> 
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.5W</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <equals>
          <property>instrumentation/cdu/display</property>
          <value>PRG-PAGE[3]</value>
        </equals>
			</and>					
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>annun.6W</object-name>
		<condition>
      <and>
        <property>systems/electrical/outputs/fms1</property>
        <or> 
          <property>autopilot/locks/fms-app</property>
          <property>autopilot/locks/fms-gs</property>
        </or> 
			</and>					
    </condition>
  </animation>

  <animation>
    <type>material</type>
    <object-name>CDU</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/flood-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>BTN</object-name>
    <emission>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
        <factor-prop>controls/lighting/ctr-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Annun</object-name>
    <condition>
     	<property>systems/electrical/outputs/lh-panel</property>
    </condition>
    <emission>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
        <factor-prop>controls/lighting/cdu</factor-prop>
    </emission>
  </animation>

<!-- CDU Knob -->

  <animation>
    <type>rotate</type>
    <object-name>CDU-knob</object-name>
    <property>controls/lighting/cdu</property>
    <factor>-270</factor>
    <center>
        <x-m>0</x-m>
        <y-m>0.0632</y-m>
        <z-m>-0.005</z-m>
    </center>
    <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>CDU-knob</object-name>
    <action>
      <button>0</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
          <command>property-adjust</command>
          <property>controls/lighting/cdu</property>
          <step>0.05</step>
          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
      </binding>
			<binding>
				<condition>
					<greater-than>
            <property>controls/lighting/cdu</property>
						<value>0.05</value>
					</greater-than>
				</condition>
				<command>property-assign</command>
					<property>instrumentation/cdu/init</property>
					<value>0</value>
			</binding>				
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>CDU-knob</object-name>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
          <command>property-adjust</command>
          <property>controls/lighting/cdu</property>
          <step>-0.05</step>
          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
      </binding>
			<binding>
				<condition>
					<less-than>
            <property>controls/lighting/cdu</property>
						<value>0.05</value>
					</less-than>
				</condition>
				<command>property-assign</command>
					<property>instrumentation/cdu/init</property>
					<value>1</value>
			</binding>				
    </action>
  </animation>

