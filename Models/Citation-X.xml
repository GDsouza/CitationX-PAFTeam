  <!-- Multiplayer events -->
  <nasal>
    <load><![CDATA[
	##############################################################################
	# The on-load Nasal is not executed when this file is loaded as the user
	# aircraft.
	##############################################################################
	var rplayer = cmdarg();

	# Set up property aliases for animations.
		rplayer.getNode("/systems/electrical/outputs/strobe", 1).alias(rplayer.getNode("sim/multiplay/generic/int[0]"));
		rplayer.getNode("/systems/electrical/outputs/beacon", 1).alias(rplayer.getNode("sim/multiplay/generic/int[1]"));
		rplayer.getNode("/systems/electrical/outputs/taxi-lights", 1).alias(rplayer.getNode("sim/multiplay/generic/int[2]"));
		rplayer.getNode("/systems/electrical/outputs/landing-light[0]", 1).alias(rplayer.getNode("sim/multiplay/generic/int[3]"));
		rplayer.getNode("/systems/electrical/outputs/landing-light[1]", 1).alias(rplayer.getNode("sim/multiplay/generic/int[4]"));
		rplayer.getNode("/systems/electrical/outputs/recog-lights", 1).alias(rplayer.getNode("sim/multiplay/generic/int[5]"));
		rplayer.getNode("/systems/electrical/outputs/nav-lights", 1).alias(rplayer.getNode("sim/multiplay/generic/int[6]"));
		rplayer.getNode("/controls/cabin-door/position-norm", 1).alias(rplayer.getNode("sim/multiplay/generic/float[0]"));
			rplayer.getNode("/surface-positions/speedbrake-pos-norm", 1).alias(rplayer.getNode("sim/multiplay/generic/float[1]"));
		rplayer.getNode("/surface-positions/reverser-norm", 1).alias(rplayer.getNode("sim/multiplay/generic/float[2]"));
    ]]></load>
  </nasal>

