<!-- EXT CABIN DOOR -->

	<animation>
	<type>translate</type>
        <object-name>Cabin.inrdoor</object-name>
        <property>controls/cabin-door/position-norm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>1.0</ind><dep>0.4</dep></entry>
        </interpolation>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>-1.0</z>
        </axis>
    </animation>

    <animation>
      <type>pick</type>
      <object-name>Cabin.inrdoor</object-name>
      <visible>true</visible>
      <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <condition>
          <property>systems/electrical/outputs/cabin-door-monitor</property>
        </condition>
        <binding>
          <command>property-toggle</command>
          <property>controls/cabin-door/open</property>
        </binding>
      </action>
    </animation> 

    <animation>
        <type>rotate</type>
        <object-name>Cabin.inrdoor</object-name>
        <property>controls/cabin-door/position-norm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>1.0</ind><dep>140.0</dep></entry>
        </interpolation>
        <center>
            <x-m>-5.594</x-m>
            <y-m>-0.813</y-m>
            <z-m>-0.468</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

<!-- CABIN SEPARATION DOORS -->
 
   <animation>
      <type>pick</type>
      <object-name>L-cabin-door</object-name>
      <object-name>R-cabin-door</object-name>
      <visible>true</visible>
			<action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
					<condition>
            <and>
						  <equals>
		            <property>controls/separation-door/open</property>						
							  <value>1.0</value>
						  </equals>
	            <property>systems/electrical/outputs/cabin-door-monitor</property>
            </and>              
					</condition>
		         <command>property-interpolate</command>
		          <property>controls/separation-door/open</property>
							<value type="double">0.0</value>
					<time>2</time>
        </binding>
        <binding>
					<condition>
						<equals>
		          <property>controls/separation-door/open</property>						
							<value>0.0</value>
						</equals>
					</condition>
		         <command>property-interpolate</command>
		          <property>controls/separation-door/open</property>
							<value type="double">1.0</value>
					<time>2</time>
        </binding>
			</action>
    	<hovered>
				<binding>
          <command>set-tooltip</command>
            <tooltip-id>Separation-doors</tooltip-id>
            <label>Separation doors: %s</label>
            <mapping>open-close</mapping>
       			<property>controls/separation-door/open</property>
        </binding>
      </hovered>
    </animation> 

    <animation>
        <type>translate</type>
        <object-name>L-cabin-door</object-name>	
        <property>controls/separation-door/open</property>
				<factor>0.15</factor>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>R-cabin-door</object-name>	
        <property>controls/separation-door/open</property>
				<factor>0.14</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

