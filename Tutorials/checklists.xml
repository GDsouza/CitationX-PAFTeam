<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

	<checklist>  
    <title>APU start</title>
			<item>
      	<name>Master</name>
      	<value>ON</value>
				<marker>
			    <x-m>-7.4463</x-m>
			    <y-m>0.6950</y-m>
			    <z-m>-0.1355</z-m>
			    <scale>2.1</scale>
				</marker>
				<condition>
						<property>controls/APU/master</property>
				</condition>
				<binding>
					<command>property-toggle</command>
					<property>controls/APU/master</property>
				</binding>
			</item>

			<item>
				<name>Start</name>
				<value>ON</value>
				<marker>
			    <x-m>-7.5083</x-m>
			    <y-m>0.6950</y-m>
			    <z-m>-0.1355</z-m>
			    <scale>2.1</scale>
				</marker>
				<condition>
          <property>controls/APU/running</property>
				</condition>
				<binding>
          <command>property-toggle</command>
            <property>controls/APU/running</property>
 				</binding>
			</item>

			<item>
				<name>            					Wait for</name>
				<value>READY TO LOAD</value>
				<marker>
			    <x-m>-7.4884</x-m>
			    <y-m>0.6950</y-m>
			    <z-m>-0.000</z-m>
			    <scale>2.1</scale>
				</marker>
					<condition>
						<greater-than> 
           		<property>controls/APU/rpm</property>
								<value>0.99</value>
						</greater-than>	
					</condition>
			</item>

			<item>
				<name>Generator</name>
				<value>ON</value>
				<marker>
			    <x-m>-7.4463</x-m>
			    <y-m>0.6950</y-m>
			    <z-m>-0.0017</z-m>
			    <scale>2.1</scale>
				</marker>
					<condition>
						<and>
							<greater-than> 
	           		<property>controls/APU/rpm</property>
								<value>0.99</value>
							</greater-than>	
							<property>controls/electric/APU-generator</property>
						</and>
					</condition>
				<binding>
          <command>property-toggle</command>
            <property>controls/electric/APU-generator</property>
 				</binding>
			</item>

			<item>
				<name>Bleed air</name>
				<value>ON</value>
				<marker>
			    <x-m>-7.5372</x-m>
			    <y-m>0.6975</y-m>
			    <z-m>-0.0</z-m>
			    <scale>2.1</scale>
				</marker>
				<condition>
					<property>controls/APU/bleed</property>
				</condition>
				<binding>
          <command>property-toggle</command>
            <property>controls/APU/bleed</property>
 				</binding>
			</item>

	</checklist>  

	<checklist>  
    <title>Engines start</title>
			<item>
				<name>Preflight check</name>	
    		<value>COMPLETED</value>
			</item>

			<item>
      	<name>Park brake</name>
      	<value>ON</value>
				<marker>
			    <x-m>-6.7851</x-m>
			    <y-m>-0.0339</y-m>
			    <z-m>-0.3099</z-m>
			    <scale>4.1074</scale>
				</marker>
	     	<condition>
						<property>controls/gear/brake-parking</property>
      	</condition>
	      <binding>
	        <command>property-interpolate</command>
	        <property>controls/gear/brake-parking</property>
	        <value>1</value>
					<time>2.0</time>
	      </binding>
			</item>

			<item>
				<name>Throttles</name>
				<value>IDLE</value>
				<marker>
			    <x-m>-7.2438</x-m>
			    <y-m>0.0198</y-m>
			    <z-m>-0.2017</z-m>
			    <scale>3.95</scale>
				</marker>
				<condition>
					<and>
						<greater-than-equals>
							<property>controls/engines/engine[0]/throttle</property>
								<value>0</value>
						</greater-than-equals>
						<greater-than-equals>
							<property>controls/engines/engine[1]/throttle</property>
								<value>0</value>
						</greater-than-equals>
					</and>
				</condition>
				<binding>
					<command>property-interpolate</command>
					<property>controls/engines/engine[0]/throttle</property>
						<value>0</value>
						<time>2.0</time>
				</binding>
				<binding>
					<command>property-interpolate</command>
					<property>controls/engines/engine[1]/throttle</property>
					<value>0</value>
					<time>2.0</time>
				</binding>
			</item>

