    <menubar>
      <default>
        <menu n="10">
          <label>Citation X</label>
          <enabled type="bool">true</enabled>
 
         <item>
            <label>Autostart</label>
            <binding>
              <command>property-toggle</command>
              <property>sim/model/autostart</property>
            </binding>
          </item>

          <item>
            <label>Options</label>
            <binding>
              <command>nasal</command>
              <script>dialogs.options.open()</script>
            </binding>
          </item>
 
         <item>
            <label>Select Livery</label>
            <binding>
                <command>nasal</command>
                <script>aircraft.livery.dialog.toggle()</script>
            </binding>
          </item>

          <item>
  					<label>Ground Services</label>
		        <binding>
      			  <command>nasal</command>
      			  <script>var ground_services = gui.Dialog.new("sim/gui/dialogs/ground-services/dialog", "Systems/ground-services.xml");
			   ground_services.open();</script>
      		  </binding>
      	  </item>

          <item>
            <label>Charts</label>
            <binding>
              <command>nasal</command>
              <script><![CDATA[charts.Charts_key();]]></script>
            </binding>
          </item>
          <item>
            <label>Engines Fire</label>
            <binding>
              <command>property-toggle</command>
              <property>controls/fire/engines-fire</property>
            </binding>
          </item>

        </menu>
     </default>
    </menubar>

