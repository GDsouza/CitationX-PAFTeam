<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

<!-- ====== FD enable ====== -->
  <logic>
    <name>FD status</name>
    <input>
      <or>
        <equals>
          <property>/autopilot/locks/heading</property>
          <value>HDG</value>
        </equals>
        <equals>
          <property>/autopilot/locks/heading</property>
          <value>LOC</value>
        </equals>
        <equals>
          <property>/autopilot/locks/heading</property>
          <value>LNAV</value>
        </equals>
        <equals>
          <property>/autopilot/locks/heading</property>
          <value>VOR</value>
        </equals>
        <equals>
          <property>/autopilot/locks/altitude</property>
          <value>ALT</value>
        </equals>
        <equals>
          <property>/autopilot/locks/altitude</property>
          <value>VALT</value>
        </equals>
        <property>/autopilot/locks/to-ga</property>
      </or>
    </input>
    <output>autopilot/locks/FD-status</output>
  </logic>

<!-- ======  Lateral Modes  ====== -->

  <filter>
    <name>nav2 heading error computer</name>
    <debug>false</debug>
    <type>gain</type>
    <gain>1.0</gain>
    <input>
      <property>instrumentation/nav[1]/radials/target-auto-hdg-deg</property>
      <offset>
        <property>orientation/heading-deg</property>
        <scale>-1.0</scale>
      </offset>
    </input>
    <output>autopilot/internal/nav2-heading-error-deg</output>
    <period>
      <min>-180</min>
      <max>180</max>
    </period>
  </filter>

  <pi-simple-controller>
      <name>Heading Bug</name>
      <debug>false</debug>
      <enable>
        <condition>
          <equals>
            <property>autopilot/locks/heading</property>
            <value>HDG</value>
          </equals>
        </condition>
      </enable>
      <input>autopilot/internal/heading-bug-error-deg</input>
      <reference>0.0</reference>
      <output>autopilot/internal/roll-filter</output>
      <config>
          <Kp>-1.5</Kp>
          <Ki>0.0</Ki>
          <min>
            <property>autopilot/settings/bank-limit</property>
            <scale>-1</scale>
          </min>
          <max>
            <property>autopilot/settings/bank-limit</property>
          </max>
      </config>
  </pi-simple-controller>

  <pi-simple-controller>
      <name>VOR-LOC</name>
      <debug>false</debug>
      <enable>
        <condition>
          <and>
            <or>
              <equals>
      		      <property>autopilot/locks/heading</property>
      		      <value>VOR</value>
              </equals>
              <equals>
      		      <property>autopilot/locks/heading</property>
      		      <value>LOC</value>
              </equals>
            </or>
            <not>
              <property>autopilot/settings/fms</property>
            </not>
          </and>
        </condition>
      </enable>
      <input>
        <condition>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV1</value>
          </equals>
        </condition>              
        <property>autopilot/internal/nav1-heading-error-deg</property>
      </input>
      <input>
        <condition>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV2</value>
          </equals>
        </condition>              
        <property>autopilot/internal/nav2-heading-error-deg</property>
      </input>
      <reference>0.0</reference>
      <output>autopilot/internal/roll-filter</output>
      <config>
          <Kp>-1.5</Kp>
          <Ki>0.0</Ki>
          <min>
            <property>autopilot/settings/bank-limit</property>
            <scale>-1</scale>
          </min>
          <max>
            <property>autopilot/settings/bank-limit</property>
          </max>
      </config>
  </pi-simple-controller>

  <pi-simple-controller>
      <name>Lnav-GS</name>
      <debug>false</debug>
      <enable>
        <condition>
          <and>
            <or>
              <equals>
	              <property>autopilot/locks/heading</property>
	              <value>LNAV</value>
              </equals>
              <equals>
	              <property>autopilot/locks/heading</property>
	              <value>LOC</value>
              </equals>
            </or>
            <property>autopilot/settings/fms</property>
          </and>
        </condition>
      </enable>
      <input>autopilot/internal/course-offset</input>
      <reference>0.0</reference>
      <output>autopilot/internal/roll-filter</output>
      <config>
          <Kp>-1.5</Kp>
          <Ki>0.0</Ki>
          <min>
            <property>autopilot/settings/bank-limit</property>
            <scale>-1</scale>
          </min>
          <max>
            <property>autopilot/settings/bank-limit</property>
          </max>
      </config>
  </pi-simple-controller>

  <filter>
      <name>Roll command</name>
      <type>noise-spike</type>
      <debug>false</debug>
      <feedback-if-disabled>true</feedback-if-disabled>
      <initialize-to>output</initialize-to>
      <enable>
        <condition>
          <equals>  
             <property>autopilot/locks/FD-status</property>
            <value>1</value>
          </equals>
        </condition>
      </enable>
      <input>autopilot/internal/roll-filter</input>
      <output>autopilot/internal/roll-bars</output>
      <max-rate-of-change>10</max-rate-of-change>
  </filter>

  <pid-controller>
      <name>Aileron Control</name>
      <debug>false</debug>
      <enable>
        <condition>
          <equals>
            <property>autopilot/locks/AP-status</property>
            <value>AP</value>
          </equals>
        </condition>
      </enable>
      <input>orientation/roll-deg</input>
      <reference>autopilot/internal/roll-bars</reference>
      <output>controls/flight/aileron</output>
      <config>
          <Kp>0.50</Kp>
          <beta>1.0</beta>
          <alpha>0.1</alpha>
          <gamma>0.0</gamma>
          <Ti>10.0</Ti>
          <Td>0.00001</Td>
          <min>-1</min>
          <max>1</max>
      </config>
  </pid-controller>

<!-- ======= Vertical Modes ======= -->
  <filter>
      <name>PTCH-TOGA</name>
      <type>noise-spike</type>
      <debug>false</debug>
      <feedback-if-disabled>false</feedback-if-disabled>
      <initialize-to>output</initialize-to>
      <enable>
        <condition>
          <or>
            <and>
              <equals>
                <property>autopilot/locks/altitude</property>
                <value>PTCH</value>
              </equals>
              <equals>
                <property>autopilot/locks/AP-status</property>
                <value>AP</value>
              </equals>
            </and>
            <equals>
              <property>autopilot/locks/altitude</property>
              <value>TO</value>
            </equals>
            <equals>
              <property>autopilot/locks/altitude</property>
              <value>GA</value>
            </equals>
          </or>
        </condition>
      </enable>
      <input>
        <property>autopilot/settings/target-pitch-deg</property>
        <max>19</max>
        <min>-15</min>
      </input>
      <output>autopilot/internal/pitch-filter</output>
      <max-rate-of-change>10</max-rate-of-change>
  </filter>

  <pid-controller>
      <name>Altitude 1</name>
      <debug>false</debug>
      <enable>
        <condition>
          <and>
            <equals>
              <property>autopilot/locks/AP-status</property>
              <value>AP</value>
            </equals>
            <or>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>ALT</value>
              </equals>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>VALT</value>
              </equals>
            </or>
            <not>
              <property>autopilot/locks/fms-gs</property>
            </not>
            <not>
              <property>autopilot/locks/fms-app</property>
            </not>
          </and>
        </condition>
      </enable>
      <input>instrumentation/altimeter/indicated-altitude-ft</input>
      <reference>autopilot/settings/tg-alt-ft</reference>
      <output>autopilot/settings/target-climb-rate-fps</output>
      <config>
          <Kp>0.50</Kp> 
          <beta>1.0</beta>
          <alpha>0.1</alpha>
          <gamma>0.0</gamma>
          <Ti>10.0</Ti> 
          <Td>0.00001</Td>
          <min>
            <property>autopilot/settings/fps-limit</property>
          </min>
          <max>70</max>
      </config>
  </pid-controller>

  <filter>
    <name>GS</name>
    <debug>false</debug>
    <type>gain</type>
    <gain>1.0</gain>
    <enable>
      <condition>
        <and>
          <equals>
            <property>autopilot/locks/altitude</property>
            <value>GS</value>
          </equals>
          <or>
            <equals>
              <property>autopilot/settings/nav-source</property>
              <value>NAV1</value>
            </equals>
            <equals>
              <property>autopilot/settings/nav-source</property>
              <value>NAV2</value>
            </equals>
            <property>autopilot/settings/fms</property>
          </or>
        </and>
      </condition>
    </enable>
    <input>
      <condition>
        <or>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV1</value>
          </equals>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>FMS1</value>
          </equals>
        </or>
      </condition>
      <property>instrumentation/nav/gs-rate-of-climb</property>
    </input>
    <input>
      <condition>
        <or>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV2</value>
          </equals>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>FMS2</value>
          </equals>
        </or>
      </condition>
      <property>instrumentation/nav[1]/gs-rate-of-climb</property>
    </input>
    <output>autopilot/settings/target-climb-rate-fps</output>
  </filter>

  <pid-controller>
    <name>FMS without GS</name>
    <debug>false</debug>
    <enable>
      <condition>
        <property>autopilot/locks/fms-app</property>
      </condition>
    </enable>
    <input>
      <property>autopilot/settings/target-pitch-deg</property>
      <scale>10</scale>
    </input>
    <reference>
      <property>autopilot/settings/descent-angle</property>
      <scale>-10</scale>
    </reference>
    <output>autopilot/internal/pitch-filter</output>
    <config>
      <Kp>-1</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>20.0</Ti>
      <Td>0.000001</Td>
      <min>-30</min>
      <max>30</max>
    </config>
  </pid-controller>

  <pid-controller>
      <name>Altitude 2</name>
      <debug>false</debug>
      <enable>
        <condition>
          <and>
            <equals>
              <property>autopilot/locks/AP-status</property>
              <value>AP</value>
            </equals>
            <not>
              <property>/autopilot/locks/fms-app</property>
            </not>
            <or>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>ALT</value>
              </equals>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>VALT</value>
              </equals>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>GS</value>
              </equals>
              <equals>
                <property>/autopilot/locks/altitude</property>
                <value>VS</value>
              </equals>
            </or>
          </and>
        </condition>
      </enable>
      <input>velocities/vertical-speed-fps</input>
      <reference>autopilot/settings/target-climb-rate-fps</reference>
      <output>autopilot/internal/pitch-filter</output>
      <config>
        <Kp>0.15</Kp>
        <beta>1.0</beta>
        <alpha>0.1</alpha>
        <gamma>0.0</gamma>
        <Ti>30.0</Ti>
        <Td>0.000001</Td>
        <min>-10</min>
        <max>10</max>
      </config>
  </pid-controller>

  <pid-controller>
      <name>flc 1</name>
      <debug>false</debug>
      <enable>
        <condition>
          <or>
            <equals>
              <property>/autopilot/locks/altitude</property>
              <value>FLC</value>
            </equals>
            <equals>
              <property>/autopilot/locks/altitude</property>
              <value>VFLC</value>
            </equals>
          </or>
        </condition>
      </enable>
      <input>instrumentation/altimeter/indicated-altitude-ft</input>
      <reference>
					<property>autopilot/settings/asel</property>
          <scale>100</scale>
      </reference>
      <output>autopilot/settings/target-climb-rate-fps</output>
      <config>
          <Kp>0.20</Kp>
          <beta>1.0</beta>
          <alpha>0.1</alpha>
          <gamma>0.0</gamma>
          <Ti>10.0</Ti> 
          <Td>0.000001</Td>
          <min>-45</min>
          <max>65</max>
      </config>
  </pid-controller>

  <pi-simple-controller>
      <name>flc 2</name>
      <debug>false</debug>
      <enable>
        <condition>
          <or>
            <equals>
              <property>/autopilot/locks/altitude</property>
              <value>FLC</value>
            </equals>
            <equals>
              <property>/autopilot/locks/altitude</property>
              <value>VFLC</value>
            </equals>
          </or>
        </condition>
      </enable>
      <input>velocities/vertical-speed-fps</input>
      <reference>autopilot/settings/target-climb-rate-fps</reference>
      <output>autopilot/internal/pitch-filter</output>
      <config>
          <Kp>0.30</Kp>
          <Ki>0.0</Ki>
          <min>-30</min>
          <max>30</max>
      </config>
  </pi-simple-controller>

  <filter>
      <name>pitch command</name>
      <type>noise-spike</type>
      <debug>false</debug>
      <feedback-if-disabled>true</feedback-if-disabled>
      <initialize-to>output</initialize-to>
      <input>autopilot/internal/pitch-filter</input>
      <output>autopilot/internal/pitch-bars</output>
      <max-rate-of-change>5</max-rate-of-change>
  </filter>

  <pid-controller>
      <name>Elevator Trim Control</name>
      <debug>false</debug>
      <enable>
        <condition>
          <or>
            <equals>
              <property>autopilot/locks/AP-status</property>
              <value>AP</value>
            </equals>
            <and>
              <property>autopilot/locks/to-ga</property>
              <greater-than>
                <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                <property>controls/flight/v1</property>
              </greater-than>
            </and>
          </or>
        </condition>
      </enable>
      <input>orientation/pitch-deg</input>
      <reference>autopilot/internal/pitch-bars</reference>
      <output>controls/flight/elevator</output>
      <config>
        <Kp>-0.025</Kp> <!-- old -0.015 -->
        <beta>1.0</beta>
        <alpha>0.1</alpha>
        <gamma>0.0</gamma>
        <Ti>10.0</Ti>	
        <Td>0.01</Td> 
        <min>-1</min>
        <max>1</max>
      </config>
  </pid-controller>

<!-- Glide Deflection -->

  <filter>
    <name>gs deflection</name>
    <type>noise-spike</type>
    <debug>false</debug>
    <feedback-if-disabled>true</feedback-if-disabled>
    <initialize-to>output</initialize-to>
    <enable>
      <equals>
        <property>/autopilot/locks/altitude</property>
        <value>GS</value>
      </equals>
    </enable>
    <input>
      <condition>
        <or>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV1</value>
          </equals>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>FMS1</value>
          </equals>
        </or>
      </condition>
      <property>instrumentation/nav/gs-needle-deflection-deg</property>
    </input>
    <input>
      <condition>
        <or>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>NAV2</value>
          </equals>
          <equals>
            <property>autopilot/settings/nav-source</property>
            <value>FMS2</value>
          </equals>
        </or>
      </condition>
      <property>instrumentation/nav[1]/gs-needle-deflection-deg</property>
    </input>
    <output>autopilot/internal/gs-deflection</output>
    <max-rate-of-change>10</max-rate-of-change>
  </filter>

<!-- Speed Control  -->

  <pid-controller>
    <name>flc kt</name>
    <debug>false</debug>
    <enable>
      <condition>
				<and>
          <or>
            <equals>
                <property>/autopilot/locks/altitude</property>
                <value>FLC</value>
            </equals>
            <equals>
                <property>/autopilot/locks/altitude</property>
                <value>VFLC</value>
            </equals>
          </or>
					<less-than>
						<property>instrumentation/altimeter/indicated-altitude-ft</property>
						<value>30650</value>
					</less-than>
				</and>
      </condition>
    </enable>
    <input>velocities/airspeed-kt</input>
    <reference>autopilot/settings/target-speed-kt</reference>
    <output>
			<property>controls/engines/engine[0]/throttle</property>
			<property>controls/engines/engine[1]/throttle</property>		
    </output>
    <config>
      <Kp>0.05</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>10</Ti>
      <Td>0.001</Td>
      <min>0.0</min>
      <max>0.95</max>
    </config>
  </pid-controller>

  <pid-controller>
    <name>flc mach</name>
    <debug>false</debug>
    <enable>
      <condition>
				<and>
          <or>
              <equals>
                  <property>/autopilot/locks/altitude</property>
                  <value>FLC</value>
              </equals>
              <equals>
                  <property>/autopilot/locks/altitude</property>
                  <value>VFLC</value>
              </equals>
          </or>
					<greater-than-equals>
						<property>instrumentation/altimeter/indicated-altitude-ft</property>
						<value>30650</value>
					</greater-than-equals>
				</and>
      </condition>
    </enable>
    <input>instrumentation/airspeed-indicator/indicated-mach</input>
    <reference>autopilot/settings/target-speed-mach</reference>
    <output>
			<property>controls/engines/engine[0]/throttle</property>
			<property>controls/engines/engine[1]/throttle</property>		
    </output>
    <config>
      <Kp>10</Kp> <!-- old 0.05 -->
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>10</Ti>   <!-- old 10 -->
      <Td>0.001</Td> <!-- old 0.001 -->
      <min>0.0</min>
      <max>0.95</max>
    </config>
  </pid-controller>

  <pid-controller>
    <name>Speed Control kt</name>
    <debug>false</debug>
		<enable>
			<condition>
				<and>
				  <equals>
					  <property>/autopilot/locks/AP-status</property>
						  <value>AP</value>
				  </equals>
				  <not>
					  <property>autopilot/locks/alt-mach</property>
				  </not>
					<or>
						<and>
<!--                <property>autopilot/settings/fms</property> -->
							<or>
								<equals>
									<property>/autopilot/locks/altitude</property>
									<value>VALT</value>
								</equals>
								<equals>
									<property>/autopilot/locks/altitude</property>
									<value>GS</value>
								</equals>
							</or>
						</and>
						<property>/autopilot/locks/speed-ctrl</property>
					</or>
				</and>
			</condition>
		</enable>
		<input>velocities/airspeed-kt</input>
		<reference>autopilot/settings/target-speed-kt</reference>
		<output>
			<property>controls/engines/engine[0]/throttle</property>
			<property>controls/engines/engine[1]/throttle</property>		
		</output>
    <config>
      <Kp>0.1</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>5.0</Ti>
      <Td>0.001</Td>
      <min>0.0</min>
      <max>0.95</max>
    </config>
  </pid-controller>

  <pid-controller>
    <name>Speed Control Toga</name>
    <debug>false</debug>
		<enable>
			<condition>
        <and>
				  <property>/autopilot/locks/to-ga</property>
					<property>/autopilot/locks/speed</property>
          <greater-than>
             <property>position/altitude-agl-ft</property>
             <value>50</value>
          </greater-than>
				</and>
			</condition>
		</enable>
		<input>velocities/airspeed-kt</input>
		<reference>autopilot/settings/target-speed-kt</reference>
		<output>
			<property>controls/engines/engine[0]/throttle</property>
			<property>controls/engines/engine[1]/throttle</property>		
		</output>
    <config>
      <Kp>0.1</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>5.0</Ti>
      <Td>0.001</Td>
      <min>0.0</min>
      <max>0.90</max>
    </config>
  </pid-controller>

  <pid-controller>
      <name>Speed Control mc</name>
      <debug>false</debug>
			<enable>
				<condition>
				<and>
					<equals>
						<property>/autopilot/locks/AP-status</property>
							<value>AP</value>
					</equals>
						<property>autopilot/locks/alt-mach</property>
					<or>
						<and>
              <property>autopilot/settings/fms</property>
							<or>
								<equals>
									<property>/autopilot/locks/altitude</property>
									<value>VALT</value>
								</equals>
								<equals>
									<property>/autopilot/locks/altitude</property>
									<value>GS</value>
								</equals>
							</or>
						</and>
						<property>/autopilot/locks/speed-ctrl</property>
					</or>
				</and>
				</condition>
			</enable>
			<input>instrumentation/airspeed-indicator/indicated-mach</input>
			<reference>autopilot/settings/target-speed-mach</reference>
			<output>
				<property>controls/engines/engine[0]/throttle</property>
				<property>controls/engines/engine[1]/throttle</property>		
			</output>
      <config>
        <Kp>5</Kp> <!-- old 10 -->
        <beta>1.0</beta>
        <alpha>0.1</alpha>
        <gamma>0.0</gamma>
        <Ti>10.0</Ti>	<!-- old 10.0 -->
        <Td>0.001</Td>
        <min>0.0</min>
        <max>0.95</max>
      </config>
  </pid-controller>

<!-- Bearing -->

  <pi-simple-controller>
    <name>NAV1 bearing </name>
    <debug>false</debug>
    <enable>
      <condition>
        <equals>
          <property>/instrumentation/sc840/nav1ptr</property>
          <value>1</value>
        </equals>
      </condition>
    </enable>
    <input>orientation/heading-magnetic-deg</input>
		<reference>instrumentation/nav/heading-deg</reference>
		<output>autopilot/internal/nav1-pointer</output>
		<config>
			<Kp>1</Kp>
			<Ki>0.0</Ki>
			<min>-360</min>
			<max>360.0</max>
		</config>
	</pi-simple-controller>

